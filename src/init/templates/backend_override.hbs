terraform { 
    backend "s3" { 
        profile = "{{terraform.aws_profile}}" 
        bucket = "{{terraform.namespace}}-{{terraform.backend_bucket_token}}"
        encrypt = true 
        dynamodb_table = "{{terraform.backend_table}}" 
        key = "terraform.tfstate"
        region = "{{terraform.aws_region}}" 
        workspace_key_prefix = "{{context}}" 
    } 
}