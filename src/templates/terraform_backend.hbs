terraform { 
    backend "s3" { 
        bucket = "{{terraform.state_bucket}}"
        encrypt = true 
        dynamodb_table = "{{terraform.state_table}}" 
        key = "{{terraform.state_key}}"
        region = "{{terraform.aws_region}}" 
        workspace_key_prefix = "{{config.context}}" 
    } 
}